def read_print(string):
    with open(string) as file:
        str = file.readline()
        gridsize = [int(s) for s in str.split() if s.isdigit()]
        if(len(gridsize) == 3):
            grid = [[[0 for x in range(gridsize[0])] for y in range(gridsize[1])] for z in range(gridsize[2])]
        else:
            print("Incorrect grid size")

        # To do: init gates at coordinates

    file.close()
    return grid

grid = read_print("print_1")
print(grid)